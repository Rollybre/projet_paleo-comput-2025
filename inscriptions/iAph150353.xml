      <div type="description" n="monument">
        <head>Description of Monument</head>
        <p>A small clay <rs type="objectType">pot</rs> (diam. <measure dim="diameter">0.105</measure> x <measure type="length" unit="metre" dim="height">0.07</measure>).</p>
      </div>

      <div type="description" n="text">
        <head>Description of Text</head>
        <p>On the body.</p>
      </div>

      <div type="description" n="letters">
        <head>Description of Letters</head>
        <p>lightly scratched: l.1, <measure type="length" unit="metre" dim="height">0.005-0.01</measure>; name, <measure type="length" unit="metre" dim="height">0.01-0.015</measure>.</p>
      </div>

      <div type="description" n="date">
        <head>Date</head>
        <p><date notBefore="0401" notAfter="0500" exact="none">Fifth century</date> or later (<rs type="criteria">content</rs>).</p>
      </div>

      <div n="text" lang="grc" type="edition">
        <head lang="en">Edition</head>
        <ab><lb n="1"/><w lemma="βοηθέω">βοΐθη</w> 
<lb n="2"/><g type="cross"/> <persName key="Theotokos" type="divine" full="yes"><w lemma="θεότοκος">θεώτοκε</w></persName> <persName key="Mary" type="divine" full="yes"><name>Μαρία</name></persName>.</ab>
      </div>

      

      <div type="translation">
        <head>Translation</head>
        <ab>Mary, mother of God, help.</ab>
      </div>

