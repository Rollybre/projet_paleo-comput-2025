		 <lb n="2"/><gap dim="left" reason="lost" extent="unknown" unit="character"/> <rs type="subject" key="acclamations"><w lemma="νικάω">νικᾷ</w></rs> <gap dim="right" reason="lost" extent="unknown" unit="character"/>
		 <lb n="3"/><gap dim="bottom" reason="lost" extent="unknown" unit="line"/>
